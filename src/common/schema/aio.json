                },
                {
                    "$ref": "#/definitions/DeezerSourceAIOConfig"
                },
                {
                    "$ref": "#/definitions/SubsonicSourceAIOConfig"
                },
                {
                    "$ref": "#/definitions/JellySourceAIOConfig"
                },
                {
                    "$ref": "#/definitions/LastFmSouceAIOConfig"
